@{
  ViewBag.Title = "Map";
}

<head>
  <meta name="description" content="Map page">
  <meta name="author" content="Xiaoyi Huang - HappyFarmer Team">
</head>

<body>
    <!-- Header -->
    <div class="header bg-gradient-primary pb-8 pt-5 pt-md-8">
      <div class="container-fluid">
        <div class="header-body">
          
          <div class="row">
            <div class="col">
              <div class="card shadow border-0">
                <div id="map-canvas" class="map-canvas" data-lat="40.748817" data-lng="-73.985428" style="height: 600px;"></div>
              </div>
            </div>
          </div>
          
          
        </div>
      </div>
    </div>
    
    <br><br><br>

    <!-- Footer -->
    <footer class="footer">
      @RenderPage("~/Views/Shared/Footer.cshtml")
    </footer>

    <!-- Page content -->
    <div class="container-fluid mt--7">
    </div>

    <!-- GoogleMap API Key -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBd3PjUqq81lIOfBPYXrQGWwK5T4ystZjA"></script>

</body>
